{
    "pretty": true,
    "lib": [
        "es2015.iterable",
        "es2015.generator",
        "es5"
    ],
    "moduleResolution": "commonjs",
    "rootDir": ".",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "composite": true,
    "noEmitOnError": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictPropertyInitialization": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "alwaysStrict": true,
    "preserveConstEnums": true,
    "newLine": "lf",
    "types": [],
    "compilerOptions": {
        "target": "es2018"
    }
}